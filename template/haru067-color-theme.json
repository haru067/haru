{
	"type": "dark",
	"colors": {
		"editor.background": "#{{base0}}",
		"foreground": "#{{base3}}",
		"editor.foreground": "#{{base3}}",
		"editorIndentGuide.background": "#{{base1}}",
		"editor.lineHighlightBackground": "#{{base1}}",
		"activityBarBadge.background": "#{{select-bri}}",
		"activityBar.background": "#{{base1}}",
		"sideBar.background": "#{{base0}}",
		"sideBar.border": "#{{base1}}",
		"statusBar.background": "#{{select-bri}}",
		"statusBar.noFolderBackground": "#{{select-bri}}",
		"statusBar.debuggingBackground": "#{{select-bri}}",
		"gitDecoration.untrackedResourceForeground": "#{{green}}",
		"gitDecoration.modifiedResourceForeground": "#{{yellow}}",
		"gitDecoration.deletedResourceForeground": "#{{red}}",
		"tab.activeForeground": "#{{base3}}",
		"panelTitle.activeForeground": "#{{base3}}",
		"terminal.ansiBlack": "#{{base0}}",
		"terminal.ansiRed": "#{{red}}",
		"terminal.ansiGreen": "#{{green}}",
		"terminal.ansiYellow": "#{{yellow}}",
		"terminal.ansiBlue": "#{{blue}}",
		"terminal.ansiMagenta": "#{{magenta}}",
		"terminal.ansiCyan": "#{{cyan}}",
		"terminal.ansiWhite": "#{{base3}}",
		"terminal.ansiBrightBlack": "#{{base2}}",
		"terminal.ansiBrightRed": "#{{orange}}",
		"terminal.ansiBrightGreen": "#{{b-green}}",
		"terminal.ansiBrightYellow": "#{{b-yellow}}",
		"terminal.ansiBrightBlue": "#{{b-blue}}",
		"terminal.ansiBrightMagenta": "#{{b-magenta}}",
		"terminal.ansiBrightCyan": "#{{b-cyan}}",
		"terminal.ansiBrightWhite": "#{{base4}}"
	},
	"tokenColors": [
		{
			"name": "Emphasis italic",
			"scope": [
				"emphasis",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Bold",
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#{{magenta}}"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#{{magenta}}"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#{{base2}}"
			}
		},
		{
			"name": "Violet",
			"scope": [
			],
			"settings": {
				"foreground": "#{{b-violet}}"
			}
		},
		{
			"name": "Offwhite",
			"scope": [
			],
			"settings": {
				"foreground": "#{{base5}}"
			}
		},
		{
			"name": "Yellow: Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"entity.name.tag",
				"punctuation.definition.tag",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded",
				"meta.selector",
                "meta.tag.sgml.doctype.html",
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.type.property-name.json",
				"support.type.property-name.media.css",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#{{yellow}}"
			}
		},
		{
			"name": "White: Variables",
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"source.python",
				"variable",
				"variable.parameter",
				"variable.other.readwrite",
				"meta.object-literal.key",
				"meta.definition.variable.name",
				"variable.language",
				"support.variable",
				"constant.character",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php",
				"text.html.basic",
				"text.xml"
			],
			"settings": {
				"foreground": "#{{base3}}"
			}
		},
		{
			"name": "Blue: SupportVariables",
			"scope": [
				"header",
				"meta.diff.header",
				"support.variable",
				"support.type.property-name",
				"markup.changed",
				"beginning.punctuation.definition.list.markdown",
				"meta.object-literal.key",
				"meta.preprocessor",
				"token.info-token"
			],
			"settings": {
				"foreground": "#{{blue}}"
			}
		},
		{
			"name": "Orange: Storages",
			"scope": [
				"storage",
				"storage.type",
				"storage.modifier",
				"variable.language",
				"constant.numeric",
				"constant.regexp",
				"constant.character",
				"constant.character.escape",
				"entity.other.attribute-name",
				"markup.inline.raw",
				"meta.preprocessor.numeric",
				"entity.name.tag.css",
				"meta.selector.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss",
				"token.warn-token"
			],
			"settings": {
				"foreground": "#{{orange}}"
			}
		},
		{
			"name": "Magenta: Keywords",
			"scope": [
				"invalid",
				"markup.deleted",
				"token.error-token",
				"constant.language",
				"keyword",
				"keyword.control",
				"keyword.operator",
				"keyword.operator.expression",
				"keyword.operator.new",
				"keyword.operator.logic",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python",
				"keyword.other.unit",
				"keyword.operator.or.regexp",
				"keyword.control.anchor.regexp",
				"keyword.operator.quantifier.regexp",
				"meta.template.expression",
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#{{magenta}}"
			}
		},
		{
			"name": "Green: Strings",
			"scope": [
				"constant.sha.git-rebase",
				"string",
				"string.regexp",
				"string.value",
				"string.tag",
				"markup.inserted",
				"beginning.punctuation.definition.quote.markdown",
				"meta.preprocessor.string",
                "keyword.other.unit.px.css",
                "keyword.other.unit.rem.css",
                "keyword.other.unit.em.css",
                "keyword.other.unit.percentage.css",
                "constant.numeric.css",
                "variable.parameter.url.css",
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp",
				"constant.character.character-class.regexp",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#{{green}}"
			}
		},
		{
			"name": "Cyan: Types",
			"scope": [
				"support.type",
				"support.class",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"meta.return-type",
				"entity.other.inherited-class",
				"entity.name.type",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy",
				"support.function.git-rebase",
				"support.type.property-name.css",
				"token.debug-token"
			],
			"settings": {
				"foreground": "#{{cyan}}"
			}
		}
	]
}